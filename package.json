{
  "name": "cms-boilerplate-root",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "backend": "cd packages/backend && npm run dev",
    "frontend:react": "cd packages/react-version && npm run start",
    "install:backend": "npm --prefix packages/backend install",
    "bootstrap": "npx lerna bootstrap -- --legacy-peer-deps",
    "start": "npm run frontend:react ; npm run backend",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"packages/**/{src, test}/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"packages/**/{src, test}/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --no-error-on-unmatched-pattern --write \"packages/**/{src,test}/**/*.{js,jsx,ts,tsx,css,md,json}\" --config .prettierrc",
    "format:check": "prettier --no-error-on-unmatched-pattern --check \"packages/**/{src,test}/**/*.{js,jsx,ts,tsx,css,md,json}\" --config .prettierrc",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "author": "AdamW",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@types/jasmine": "^4.3.1",
    "@wdio/cli": "^8.6.9",
    "@wdio/jasmine-framework": "^8.6.9",
    "@wdio/local-runner": "^8.6.9",
    "@wdio/spec-reporter": "^8.6.8",
    "autoprefixer": "^9",
    "chromedriver": "^111.0.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.13",
    "express-handlebars": "^6.0.6",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^8.0.3",
    "lerna": "^6.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-wait-for": "^3.0.2",
    "web-vitals": "^2.1.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
